{
  "name": "example-portal",
  "version": "1.2.0",
  "description": "example site made by Propellerhead",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.51",
    "@optimizely/react-sdk": "^2.0.1",
    "@react-native-community/async-storage": "^1.11.0",
    "@testing-library/dom": "^7.2.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@testing-library/user-event": "^10.0.2",
    "clsx": "^1.1.0",
    "contentful": "^7.14.4",
    "css-mediaquery": "^0.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-helmet": "^6.0.0",
    "react-markdown": "^4.3.1",
    "react-material-ui-carousel": "^1.4.5",
    "react-router-dom": "^5.1.2",
    "react-router-hash-link": "^1.2.2",
    "react-scripts": "^3.4.1",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "start": "REACT_APP_STAGE=dev react-scripts start",
    "build": "react-scripts build",
    "prodbuild": "REACT_APP_STAGE=production yarn run build",
    "test": "react-scripts test",
    "cy:open": "yarn run cypress open",
    "cy:run": "yarn run cypress run"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:jsx-a11y/recommended",
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "jsx-a11y",
      "import"
    ],
    "rules": {
      "import/order": [
        "warn",
        {
          "groups": [
            "builtin",
            "external",
            "internal"
          ],
          "pathGroups": [{
            "pattern": "react",
            "group": "external",
            "position": "before"
          }],
          "pathGroupsExcludedImportTypes": [
            "react"
          ],
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@netlify/build": "^0.4.13",
    "babel-eslint": "^10.1.0",
    "cypress": "^4.7.0",
    "cypress-axe": "^0.8.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "jest": "^24.9.0",
    "netlify-plugin-a11y": "^0.0.9",
    "prop-types": "^15.6.2",
    "react-test-renderer": "^16.13.1"
  }
}
